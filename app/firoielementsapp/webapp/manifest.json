{
    "_version": "1.49.0",
    "sap.app": {
        "id": "firoielementsapp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:lrop",
            "version": "1.8.6",
            "toolsId": "60df6c58-015d-4c1e-8dcb-4aa7e8323b6c"
        },
        "dataSources": {
            "mainService": {
                "uri": "/admin/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.110.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "firoielementsapp.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "AuthorsList",
                    "target": "AuthorsList"
                },
                {
                    "name": "RouteView2",
                    "pattern": "view2Nav",
                    "target": "RouteView2"
                }
            ],
            "targets": {
                "RouteView2": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Edit",
                    "viewName": "Edit"
                },
                "AuthorsList": {
                    "type": "Component",
                    "id": "AuthorsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Books",
                            "navigation": {},
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "columns": {
                                        "DataField::books::title": {
                                            "availability": "Default"
                                        }
                                    },
                                    "actions": {
                                        "create": {
                                            "press": "firoielementsapp.ext.controller.Create.create",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": true,
                                            "text": "Create Author"
                                        },
                                        "sadsads": {
                                            "press": "firoielementsapp.ext.controller.Dsdsdd.sdsdsd",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": false,
                                            "text": "dsadsad"
                                        },
                                        "wwdw": {
                                            "press": "firoielementsapp.ext.controller.Dsds.dsds",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": false,
                                            "text": "sdsds"
                                        },
                                        "rr": {
                                            "press": "firoielementsapp.ext.controller.Rr.rr",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": false,
                                            "text": "rr"
                                        },
                                        "rew": {
                                            "press": "firoielementsapp.ext.controller.Sd.ff",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": false,
                                            "text": "ee"
                                        },
                                        "few": {
                                            "press": "firoielementsapp.ext.controller.Dsds.dsds",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": false,
                                            "text": "fefe"
                                        },
                                        "zx": {
                                            "press": "firoielementsapp.ext.controller.Dsds.dsds",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": false,
                                            "text": "zx"
                                        },
                                        "CreateAuthor": {
                                            "press": "firoielementsapp.ext.controller.Rr.rr",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": false,
                                            "text": "Create Author"
                                        }
                                    }
                                },
                                "books/@com.sap.vocabularies.UI.v1.LineItem#tableView": {
                                    "tableSettings": {
                                        "creationMode": {
                                            "createAtEnd": true,
                                            "name": "NewPage"
                                        }
                                    }
                                },
                                "@com.sap.vocabularies.UI.v1.LineItem#tableView1": {
                                    "actions": {
                                        "dsds": {
                                            "press": "firoielementsapp.ext.controller.Sd.ff5",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": false,
                                            "text": "ds",
                                            "position": {
                                                "placement": ""
                                            }
                                        }
                                    }
                                },
                                "@com.sap.vocabularies.UI.v1.LineItem#tableView": {
                                    "actions": {
                                        "rest": {
                                            "press": "firoielementsapp.ext.controller.Sd.ff",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": false,
                                            "text": "Create"
                                        },
                                        "copy": {
                                            "press": "firoielementsapp.ext.controller.Dsds.dsds",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": true,
                                            "text": "Copy",
                                            "position": {
                                                "placement": "After",
                                                "anchor": "rest"
                                            }
                                        }
                                    }
                                }
                            },
                            "hideFilterBar": false,
                            "defaultTemplateAnnotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#tableView"
                        }
                    }
                }
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {}
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}